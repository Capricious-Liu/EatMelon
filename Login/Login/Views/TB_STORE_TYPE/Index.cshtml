@model IEnumerable<Login.Models.TB_STORE_TYPE>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <p>
        @Html.ActionLink("Create New", "Create", new { storeid = ViewData["sid"] })
    </p>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.TYPE)
            </th>
            <th></th>
        </tr>
    
    @foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.TYPE)
            </td>

            <td>
                @Html.ActionLink("delete", "Delete", new { storeid = ViewData["sid"], storeType = item.TYPE })
            </td>
        </tr>
    }
    
    </table>
</body>
</html>
